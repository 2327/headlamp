{
  "name": "headlamp-greenify",
  "version": "0.1.0",
  "main": "dist/index.js",
  "private": false,
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xxx/xxx"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "test": "react-scripts test",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:plugin && npm run build:module",
    "build:module": "cross-env NODE_ENV=production babel src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__ --source-maps inline",
    "start": "cross-env NODE_ENV=development babel --watch src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__ --source-maps inline",
    "xstart": "headlamp-plugin --watch",
    "build:plugin": "headlamp-plugin"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@kinvolk/headlamp-plugin": "^0.1.0",
    "cross-env": "^7.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2"
  }
}
